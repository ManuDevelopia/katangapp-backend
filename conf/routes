# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /paradas                    controllers.KatangappApplication.main(lt:String, ln:String, r:Integer)
GET     /store/busStops               controllers.KatangappApplication.busStops()
GET     /store/routes               controllers.KatangappApplication.routes()
GET     /unauto                    controllers.KatangappApplication.unauto(idl:String, idp:String, ido:String)

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
